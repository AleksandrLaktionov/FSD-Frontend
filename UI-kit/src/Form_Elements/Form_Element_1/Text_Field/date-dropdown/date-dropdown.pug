mixin dateDropDown(text_left, text_right)
    form(action="#" class="form-date-dropdown")
        .date-dropdown
            h3=text_left
            +formInput("text", "ДД.ММ.ГГГГ")(class="form-input masked-date")
        .date-dropdown
            h3=text_right
            +formInput("text", "ДД.ММ.ГГГГ", "19.08.2019")(class="form-input masked-date")
